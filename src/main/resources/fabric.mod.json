  "authors": [
    "Maheesha"
  ],
  "contact": {
    "homepage": "https://maheevil.github.io/",
    "sources": "https://github.com/MaheEvil/EndGrief"
  },

  "license": "MPL-2.0",
  "icon": "assets/endgrief/icon.png",

